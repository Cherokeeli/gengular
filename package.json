{
  "name": "gengular",
  "version": "1.0.0",
  "description": "general angularjs 1.6.x boilerplate",
  "main": "index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dev": "npm run build:dll && npm run build:dev:standalone",
    "build:dev:standalone": "node_modules/.bin/webpack --config webpack.development.config.js --progress --display-error-details",
    "build:dll": "npm run clean && node_modules/.bin/webpack --config webpack.dll.config.js --progress --display-error-details",
    "start:dev:local": "node_modules/.bin/webpack-dev-server --config webpack.development.config.js --open --progress --display-error-details",
    "start:dev:host": "node_modules/.bin/webpack-dev-server --config webpack.development.config.js --host 0.0.0.0 --progress --display-error-details",
    "build:prd": "npm run build:dll && node_modules/.bin/webpack --config webpack.production.config.js --progress --display-error-details",
    "clean": "rm -rf dist/",
    "pack:zip": "zip -r ./dist.zip ./dist/**",
    "pack:tar": "cd dist && tar -cvf dist.tar ./ && mv dist.tar ../",
    "generate:component": "node app/global/generator/component.js --",
    "generate:module": "node app/global/generator/module.js --"
  },
  "author": "Cherokeeli",
  "license": "ISC",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.20",
    "angular": "^1.6.10",
    "angular-animate": "^1.6.10",
    "angular-confirm1": "^1.1.0",
    "angular-file-upload": "^2.5.0",
    "angular-ui-bootstrap": "^2.5.6",
    "angular-ui-notification": "^0.3.6",
    "angular-ui-tree": "^2.22.6",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "html5-boilerplate": "^6.1.0",
    "ionicons": "^4.4.3",
    "jquery": "^3.3.1",
    "layui-laydate": "^5.0.9",
    "oclazyload": "^1.1.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-ie": "^6.7.0",
    "css-loader": "^1.0.0",
    "ejs": "^2.6.1",
    "ejs-loader": "^0.3.1",
    "file-loader": "^2.0.0",
    "happypack": "^5.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "ng-cache-loader": "0.0.26",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  }
}
